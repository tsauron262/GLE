    <!-- User session type. Used in all request message. -->
    <xs:complexType name="gsxUserSessionType">
        <xs:sequence>
            <xs:element name="userSessionId" type="xs:token" minOccurs="1" maxOccurs="1" />
        </xs:sequence>
    </xs:complexType>
    <!-- Restricted Unit Part Type -->
    <xs:complexType name="restrictedUnitPartType">
        <xs:complexContent>
            <xs:restriction base="base:baseUnitPartType">
                <xs:sequence>
                    <xs:element name="partNumber" type="basic:Token_18" minOccurs="1" />
                    <xs:element name="comptiaCode" type="basic:String_3_LOANER" minOccurs="0" />
                </xs:sequence>
            </xs:restriction>            
        </xs:complexContent>
    </xs:complexType>
    
    <!-- Extended Unit Part Type -->
    <xs:complexType name="extendedUnitPartType">
        <xs:complexContent>
            <xs:extension base="base:baseUnitPartType">
                <xs:sequence>
                    <xs:element name="comptiaModifier" type="basic:String_1_LOANER" minOccurs="0" />

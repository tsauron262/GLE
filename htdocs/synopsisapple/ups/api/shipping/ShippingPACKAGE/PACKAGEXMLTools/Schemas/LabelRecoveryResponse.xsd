<xsd:schema elementFormDefault="unqualified" xmlns:ups="http://www.ups.com/XMLSchema" xmlns:xsd="http://www.w3.org/2001/XMLSchema">  <xsd:element name="LabelRecoveryResponse">    <xsd:complexType>      <xsd:sequence>        <xsd:element ref="Response"/>        <xsd:element minOccurs="0" name="ShipmentIdentificationNumber" type="xsd:string"/>        <xsd:choice maxOccurs="unbounded" minOccurs="0">          <xsd:element name="LabelResults" type="LabelResultsType"/>          <xsd:element name="TrackingCandidate" type="TrackingCandidateType"/>        </xsd:choice>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:complexType name="LabelResultsType">    <xsd:sequence>      <xsd:element name="TrackingNumber" type="xsd:string"/>      <xsd:element name="LabelImage" type="LabelImageType"/>      <xsd:element minOccurs="0" name="Receipt" type="ReceiptType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="LabelImageType">    <xsd:sequence>      <xsd:element name="LabelImageFormat" type="CodeType"/>      <xsd:element name="GraphicImage" type="xsd:string"/>      <xsd:element minOccurs="0" name="HTMLImage" type="xsd:string"/>      <xsd:element minOccurs="0" name="PDF417" type="xsd:string"/>      <xsd:element minOccurs="0" name="InternationalSignatureGraphicImage" type="xsd:string"/>      <xsd:element minOccurs="0" name="URL" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="ReceiptType">    <xsd:sequence>      <xsd:element minOccurs="0" name="HTMLImage" type="xsd:string"/>      <xsd:element minOccurs="0" name="Image" type="ImageType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="ImageType">    <xsd:sequence>      <xsd:element minOccurs="0" name="ImageFormat" type="CodeType"/>      <xsd:element minOccurs="0" name="GraphicImage" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="CodeType">    <xsd:sequence>      <xsd:element name="Code" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="TrackingCandidateType">    <xsd:sequence>      <xsd:element name="TrackingNumber" type="xsd:string"/>      <xsd:element minOccurs="0" name="DestinationPostalCode" type="xsd:string"/>      <xsd:element minOccurs="0" name="DestinationCountryCode" type="xsd:string"/>      <xsd:element minOccurs="0" name="PickupDateRange" type="PickupDateRangeType"/>    </xsd:sequence>  </xsd:complexType>  <xsd:complexType name="PickupDateRangeType">    <xsd:sequence>      <xsd:element name="BeginDate" type="xsd:string"/>      <xsd:element name="EndDate" type="xsd:string"/>    </xsd:sequence>  </xsd:complexType>  <xsd:element name="Response">    <xsd:complexType>      <xsd:sequence>        <xsd:element minOccurs="0" ref="TransactionReference"/>        <xsd:element name="ResponseStatusCode" type="xsd:string"/>        <xsd:element minOccurs="0" name="ResponseStatusDescription" type="xsd:string"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="Error"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="TransactionReference">    <xsd:complexType>      <xsd:sequence>        <xsd:element minOccurs="0" name="CustomerContext" type="xsd:string"/>        <xsd:element minOccurs="0" name="XpciVersion" type="xsd:string"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="Error">    <xsd:complexType>      <xsd:sequence>        <xsd:element name="ErrorSeverity" type="xsd:string"/>        <xsd:element name="ErrorCode" type="xsd:string"/>        <xsd:element minOccurs="0" name="ErrorDescription" type="xsd:string"/>        <xsd:element minOccurs="0" name="MinimumRetrySeconds" type="xsd:string"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" ref="ErrorLocation"/>        <xsd:element maxOccurs="unbounded" minOccurs="0" name="ErrorDigest" type="xsd:string"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element>  <xsd:element name="ErrorLocation">    <xsd:complexType>      <xsd:sequence>        <xsd:element minOccurs="0" name="ErrorLocationElementName" type="xsd:string"/>        <xsd:element minOccurs="0" name="ErrorLocationElementReference" type="xsd:string" ups:usage="notused"/>        <xsd:element minOccurs="0" name="ErrorLocationAttributeName" type="xsd:string"/>      </xsd:sequence>    </xsd:complexType>  </xsd:element></xsd:schema>
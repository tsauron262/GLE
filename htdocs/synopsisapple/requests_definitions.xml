            <data name="repairConfirmationNumber" required="1"></data>
            <data name="partInfo" required="1">
                <datas>
                    <data name="partNumber" required="1"></data>
                    <data name="oldSerialNumber" required="1"></data>
                    <data name="serialNumber" required="1"></data>
                    <data name="reasonCode" required="0"></data>
